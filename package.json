    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "bracketSpacing": true,
    "singleQuote": true
