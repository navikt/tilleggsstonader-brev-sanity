    "date-fns": "^4.1.0",
    "globals": "^16.0.0",
    "groq": "^3.81.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "sanity": "^3.81.0",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
