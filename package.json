    "@sanity/vision": "^5.8.1",
    "date-fns": "^4.1.0",
    "globals": "^17.3.0",
    "groq": "^5.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-is": "^19.2.4",
    "sanity": "^5.8.1",
    "styled-components": "^6.3.8"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "@sanity/eslint-config-studio": "^6.0.0",
    "@types/react": "^19.2.13",
    "@types/styled-components": "^5.1.36",
